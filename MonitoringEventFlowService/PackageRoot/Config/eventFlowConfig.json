{
  "inputs": [
    {
      "type": "EventSource",
      "sources": [
        //{ "providerName": "Microsoft-ServiceFabric-Services" },
        //{ "providerName": "Microsoft-ServiceFabric-Actors" },
        { "providerName": "MyCompany-Team7-MonitoringEventFlowService" }
      ]
    },
    {
      "type": "PerformanceCounter",
      "sampleIntervalMsec": "5000",
      "counters": [
        {
          "counterCategory": "Memory",
          "counterName": "Available MBytes"
        },
        {
          "counterCategory": "Memory",
          "counterName": "Available KBytes"
        },
        {
          "counterCategory": "Memory",
          "counterName": "Available Bytes"
        },
        {
          "counterCategory": "Memory",
          "counterName": "Committed Bytes"
        },
        {
          "counterCategory": "Process",
          "counterName": "Private Bytes"
        }      
      ]
    }
  ],
      "filters": [
        {
          "type": "drop",
          "include": "Level == Verbose"
        }
      ],
      "outputs": [
